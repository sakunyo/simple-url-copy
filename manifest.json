{
  "name": "simple-url-copy",
  "version": "1.2",
  "description": "A url copy tool",
  "permissions": ["declarativeContent", "activeTab"],
  "background": {
    "scripts": ["dest/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "dest/popup.html"
  },
  "browser_action": {
    "default_icon": "dest/icon.png"
  },
  "manifest_version": 2,
  "icons": {
    "128": "dest/icon.png"
  }
}
